#ifndef RECLAMATION_H_
#define RECLAMATION_H_
typedef struct reclamation{
	char numero[20];
	char login[20];
	char type[20];
	char cont[1000];
	char etat[20];
	
	}reclamation;
void ajouter_reclamation();
void afficher_reclamation();
int supprimer_reclamation(char numero[]);
int modifier_reclamation(char numero[]);

#endif
